<h1>Editing project</h1>

<% form_for(@project) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :company_id %><br />
    <%= f.text_field :company_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
    <% if !@project.user.nil? %>
		<%= @project.user.username %>
	<% end %>
  </p>
<% end %>

<%= link_to 'Show', @project %> |
<%= link_to 'Back', @project.company %>